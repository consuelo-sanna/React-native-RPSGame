{
  "expo": {
    "name": "RPSGame",
    "slug": "RPSGame",
    "description": "Try to beat this amazing futuristic artificial intelligence in a fight to the death of Rock Paper Scissors. Just kidding, but it is always fun to play!",
    "version": "1.0.1",
    "owner": "Consuelo Sanna",
    "githubUrl": "https://github.com/consuelo-sanna/React-native-RPSGame",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "sanna.consuelo.RPSGame"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}